{
  "author": {
    "name": "Bojan D.",
    "email": "dbojan@gmail.com"
  },
  "name": "json-schema-deref-sync",
  "description": "Simple Node.js JSON Schema dereferencer",
  "keywords": [
    "json",
    "schema",
    "deref"
  ],
  "version": "0.3.4",
  "homepage": "https://github.com/bojand/json-schema-deref-sync",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/bojand/json-schema-deref-sync.git"
  },
  "bugs": {
    "url": "http://github.com/bojand/json-schema-deref-sync/issues"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "main": "dist/index.js",
  "dependencies": {
    "clone": "~2.0.0",
    "dag-map": "~1.0.0",
    "is-valid-path": "^0.1.1",
    "lodash": "^4.7.0",
    "md5": "~2.2.0",
    "memory-cache": "~0.1.5",
    "mpath": "~0.2.1",
    "valid-url": "~1.0.9",
    "traverse": "~0.6.6"
  },
  "devDependencies": {
    "async": "~2.1.0",
    "babel-cli": "~6.14.0",
    "babel-preset-es2015": "~6.14.0",
    "chai": "~3.4.1",
    "fs.extra": "~1.3.2",
    "jsdoc-to-markdown": "^1.3.3",
    "mocha": "~2.3.4"
  },
  "scripts": {
    "test": "npm run dist && mocha",
    "docs": "jsdoc2md \"lib/**/*.js\" --template readme.hbs > README.md",
    "dist": "node ./node_modules/babel-cli/bin/babel.js lib --out-dir dist",
    "prepublish": "npm run dist"
  },
  "xo": {
    "envs": [
      "node",
      "es6"
    ],
    "esnext": true,
    "space": true,
    "rules": {
      "babel/arrow-parens": 1,
      "babel/object-curly-spacing": 0,
      "babel/object-shorthand": 1,
      "brace-style": 1,
      "no-else-return": 1,
      "no-unused-vars": 1
    }
  },
  "directories": {
    "test": "test"
  }
}
